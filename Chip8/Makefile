# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  program_config = debug
endif
ifeq ($(config),release)
  program_config = release
endif

PROJECTS := program

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

program:
ifneq (,$(program_config))
	@echo "==== Building program ($(program_config)) ===="
	@${MAKE} --no-print-directory -C . -f program.make config=$(program_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f program.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   program"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"